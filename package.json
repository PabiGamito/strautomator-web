{
    "name": "strautomator-web",
    "version": "20.467.11713",
    "description": "Strautomator.com website.",
    "author": "Igor Ramadas",
    "main": "server/index.js",
    "types": "server/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/strautomator/web.git"
    },
    "scripts": {
        "build": "nuxt build",
        "generate": "nuxt generate",
        "start": "cross-env NODE_ENV=production node server/index.js",
        "start:ts": "cross-env NODE_ENV=development node --preserve-symlinks -r ts-node/register ./src/index.ts",
        "start:dev": "nodemon"
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.12.2",
        "@nuxtjs/google-analytics": "^2.4.0",
        "@nuxtjs/moment": "^1.6.1",
        "@nuxtjs/vuetify": "^1.11.2",
        "Base64": "^1.1.0",
        "body-parser": "^1.19.0",
        "chart.js": "^2.9.4",
        "client-sessions": "^0.8.0",
        "cookie-universal-nuxt": "^2.1.4",
        "cross-env": "^7.0.2",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.3",
        "nuxt": "^2.14.7",
        "qs": "^6.9.4",
        "typescript": "^4.0.5",
        "vuedraggable": "^2.24.3",
        "strautomator-core": "strautomator/core"
    },
    "devDependencies": {
        "@nuxt/types": "^2.14.7",
        "@types/express": "^4.17.9",
        "@types/node": "^14.14.7",
        "axios-debug-log": "^0.8.2",
        "chalk": "^4.1.0",
        "nodemon": "^2.0.6",
        "ts-node": "^9.0.0",
        "typedoc": "^0.19.2"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run start:ts",
        "ext": "ts",
        "delay": "5000"
    },
    "prettier": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false,
        "arrowParens": "always",
        "bracketSpacing": false,
        "printWidth": 250
    }
}
