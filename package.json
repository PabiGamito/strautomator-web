{
    "name": "strautomator-web",
    "version": "21.263.12116",
    "description": "Strautomator.com website.",
    "author": "Igor Ramadas",
    "main": "server/index.js",
    "types": "server/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/strautomator/web.git"
    },
    "scripts": {
        "build": "nuxt build",
        "generate": "nuxt generate",
        "start": "cross-env NODE_ENV=production node server/index.js",
        "start:ts": "cross-env NODE_ENV=development node --preserve-symlinks -r ts-node/register ./src/index.ts",
        "start:dev": "nodemon"
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.13.6",
        "@nuxtjs/dayjs": "^1.4.0",
        "@nuxtjs/google-analytics": "^2.4.0",
        "@nuxtjs/vuetify": "^1.12.1",
        "Base64": "^1.1.0",
        "body-parser": "^1.19.0",
        "chart.js": "^3.4.0",
        "chartjs-adapter-dayjs-3": "^1.2.2",
        "client-sessions": "^0.8.0",
        "cookie-universal-nuxt": "^2.1.5",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.5",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.6",
        "nuxt": "^2.15.7",
        "qs": "^6.10.1",
        "strautomator-core": "strautomator/core",
        "typescript": "^4.3.4",
        "vuedraggable": "^2.24.3"
    },
    "devDependencies": {
        "@nuxt/types": "^2.15.7",
        "@types/express": "^4.17.12",
        "@types/node": "^15.12.5",
        "axios-debug-log": "^0.8.2",
        "chalk": "^4.1.1",
        "nodemon": "^2.0.9",
        "ts-node": "^10.0.0"
    },
    "peerDependencies": {
        "anyhow": "^2.2.3",
        "setmeup": "^1.7.2"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run start:ts",
        "ext": "ts",
        "delay": "5000"
    },
    "prettier": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false,
        "arrowParens": "always",
        "bracketSpacing": false,
        "printWidth": 250
    }
}
