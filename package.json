{
    "name": "strautomator-web",
    "version": "20.221.10010",
    "description": "Strautomator.com website.",
    "author": "Igor Ramadas",
    "private": true,
    "main": "server/index.js",
    "types": "server/index.d.ts",
    "scripts": {
        "build": "nuxt build",
        "generate": "nuxt generate",
        "start": "cross-env NODE_ENV=production node server/index.js",
        "start:ts": "cross-env NODE_ENV=development node --preserve-symlinks -r ts-node/register ./src/index.ts",
        "start:dev": "nodemon"
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.10.3",
        "@nuxtjs/google-analytics": "^2.3.0",
        "@nuxtjs/vuetify": "^1.11.2",
        "anyhow": "^1.5.5",
        "body-parser": "^1.19.0",
        "client-sessions": "^0.8.0",
        "cookie-universal-nuxt": "^2.1.4",
        "cross-env": "^7.0.2",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.3",
        "lodash": "^4.17.15",
        "moment": "2.26.0",
        "nuxt": "^2.12.2",
        "qs": "^6.9.4",
        "setmeup": "^1.5.4",
        "strautomator-core": "strautomator/core",
        "typescript": "^3.9.3"
    },
    "devDependencies": {
        "@nuxt/types": "^0.7.6",
        "@types/express": "^4.17.6",
        "@types/node": "^14.0.5",
        "axios-debug-log": "^0.7.0",
        "chalk": "^4.0.0",
        "nodemon": "^2.0.4",
        "ts-node": "^8.10.1",
        "typedoc": "^0.17.7"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run start:ts",
        "ext": "ts",
        "delay": "5000"
    },
    "prettier": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false,
        "arrowParens": "always",
        "bracketSpacing": false,
        "printWidth": 250
    }
}
